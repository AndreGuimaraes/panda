<table id="videos">
	<tr>
		<th>Filename</th>
		<th></th>
		<th>Uploaded at</th>
	</tr>
<% videos.each do |v| %>
	<tr>
		<td>
			<a href="/videos/<%= v.key %>" class="filename"><%= v.original_filename || "No filename" %></a>
			<span class="token"><%= v.duration_str %></span>
		</td>
		<td></td>
		<td><%# v.created_at.strftime("%a, %d %b %Y %H:%M:%S") %></td>
	</tr>
	
	<% v.encodings.each do |e| %>
  	<tr>
  		<td><%= e.profile_title %></td>
  		<td></td>
  		<td class="<%= e.status %>"><%= e.status %></td>
  		<td></td>
  	</tr>
  <% end %>
<% end %>
</table>